{
  "manifest_version": 3,
  "name": "Tealium Events Debugger",
  "author": "johnson.lau@gmail.com",
  "version": "1.0",
  "description": "Inspect payloads of Tealium events sent from site.",
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "permissions": [
    "sidePanel",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}